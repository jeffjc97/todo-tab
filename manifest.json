{
  "manifest_version": 2,

  "name": "TODO",
  "description": "Replace your New Tab page with a no-frills todo list.",
  "version": "1.0.0",

  "chrome_url_overrides" : {
    "newtab": "todo.html"
  },

  "icons": { 
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png" },

  "permissions": [
    "storage"
  ]
}
